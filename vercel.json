{
    "name": "swiftearnprime",
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/server.js"
        }
    ],

    "headers": [
        { "source": "./routes/*.js", "headers": [{ "key": "Access-Control-Allow-Origin", "value": "*" }] }
      ]
}
